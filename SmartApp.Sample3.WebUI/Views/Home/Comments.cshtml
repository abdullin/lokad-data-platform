@model SmartApp.Sample3.Contracts.Sample3CommentData
<div>
    Events proccessed: @(Model != null ? Model.EventsProcessed : 0)
</div>
<div>
    Bytes proccessed: @(Model != null ? Model.NextOffset : 0)
</div>
<div>
@DateTime.Now
</div>
<table cellpadding="0" cellspacing="0" border="1" class="post">
    <tr>
        <th>UserId</th>
        <th>Number of comments</th>
    </tr>
    @if (Model != null)
    {
        foreach (var pair in Model.Distribution.OrderBy(x => x.Key))
        {
        <tr>
            <td>@pair.Key</td>
            <td>@pair.Value</td>
        </tr>
        }
    }
</table>
<br style="clear: both" />